{
  "name": "hlapps-mepdesignconsole",
  "description": "HL Apps Design Console",
  "repository": "https://github.com/willhl/GraphData-MEP-Forge-Example",
  "logo": "",
  "keywords": ["hoarelea", "forge", "node", "express"],
  "env": {
    "NODE_ENV": {
      "description": "Environment, defaulted to production",
      "value": "production"
    },
    "NPM_CONFIG_PRODUCTION": {
      "description": "This forces Heroku to install devDependencies, needed to build the App. Must be false!",
      "value": "false"
    },
    "FORGE_CLIENT_ID": {
      "description": "Forge Client ID API Key"
    },
    "FORGE_CLIENT_SECRET": {
      "description": "Forge Client Secret API Key"
    },
    "FORGE_CALLBACK_URL": {
      "description": "Forge oauth Callback URL"
    },
    "HOST_URL": {
      "description": "Host url: for Heroku App use 'https://myApp.herokuapp.com', where 'myApp' is name of your app. Important: the name of your App needs to match the callback URL you specified on Forge Developer Portal"
    },
   "NEO4J_HOST": {
      "description": "graph database host"
    },
    "NEO4J_PW": {
      "description": "graph database password"
    },
    "NEO4J_UN": {
      "description": "graph database username"
    },
    "NEO4J_PORT": {
      "description": "graph database port"
    },
    "NEO4J_PROTOCOL": {
      "description": "graph database protocol"
    }
  }
}
